{
	"http": {
		"ip": "0.0.0.0",
		"port": 8000
	},
	"admin": {
		"username": "root",
		"password": "password"
	},
	"rateLimit": {
		"windowMs": 300000,
		"max": 500,
		"standardHeaders": true,
		"legacyHeaders": true
	},
	"cors": {
		"origin": "*",
		"methods": [
			"GET",
			"PUT",
			"PATCH",
			"POST",
			"DELETE",
			"OPTIONS",
			"HEAD"
		],
		"preflightcontinue": false
	},
	"bodyParser": {
		"inflate": true,
		"limit": "1gb",
		"reviver": true,
		"type": "*/json"
	},
	"csp": {
		"defaultSrc": [
			"'self'"
		],
		"scriptSrc": [
			"'self'",
			"'unsafe-eval'"
		],
		"upgradeInsecureRequests": null
	}
}
